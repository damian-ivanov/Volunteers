@model CompleteProjectFormModel

@{
    ViewData["Title"] = "Complete project";
}

<h4>Complete project</h4>
<hr />
<div class="row">
    <div class=".col-md-6 align-self-center">
        <form asp-action="Complete" EncType="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="form-group">
                <label asp-for="CompletedImage" class="control-label"></label>
                <input required type="file" name="Image" class="form-control-file" />
                <span asp-validation-for="CompletedImage" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Finish!" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a href="javascript:history.back()">Go Back</a>
</div>